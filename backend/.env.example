# Rename this file to .env and adjust values for your environment.
APP_NAME=SvelteFastAPI
API_PREFIX=/api
SECRET_KEY=change-me
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=15
REFRESH_TOKEN_EXPIRE_DAYS=7
BACKEND_CORS_ORIGINS=http://localhost:5173
ALLOW_CORS_CREDENTIALS=true
ALLOW_CORS_METHODS=GET,POST,PUT,PATCH,DELETE,OPTIONS
ALLOW_CORS_HEADERS=Authorization,Content-Type,X-CSRF-Token
COOKIE_DOMAIN=
COOKIE_SECURE=false
COOKIE_SAMESITE=lax
DATABASE_URL=postgresql+psycopg://postgres:postgres@db:5432/postgres
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0
REMINDER_INTERVAL_MINUTES=60
